Q5
#include <stdio.h>
int main() {
int a = 20,b = 10,c;
c = a+b
printf("a+b =%d,c");
c = a-b
printf("a*b =%d,c");
c = a*b
printf("a/b =%d,c");
c = a/b
printf("a+b =%d,c");
return 0;
}
