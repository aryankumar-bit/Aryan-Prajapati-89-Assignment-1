Q8
#include <stdio.h>
int main() {
float dollars, rupees;
printf("Enter the amount in dollars");
scanf("%f", &dollars);
rupees = dollars*48;
printf("%f dollars is equal to %f  rupees", dollars, rupees);
return 0;
}
