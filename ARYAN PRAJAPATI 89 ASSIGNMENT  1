}

Q13
#include <stdio.h>
float main() {
float byte, KB,  MB, KB;
printf("Enter the amount in bytes");
scanf("%f",&bytes);
KB = bytes / 1024;
printf("%f bytes is equal to %f KB,bytes,KB );
MB = bytes / 1048576;
printf("%f bytes is equal to %f MB,bytes,MB );
GB = bytes / 1073741824;
printf("%f bytes is equal to %f GB,bytes,GB );
return 0;
}

Q14
#include <stdio.h>
float main() {
float C, F;
printf("Enter the temperature in C");
scanf("%f", &C);
F = 9/5*C + 32;
printf("%f celsius is equal to %f fahrenheit", C, F);
return 0;
}

Q15
#include <stdio.h>
float main() {
float C, F;
printf("Enter the temperature in F");
scanf("%f", &F);
C = (F - 32) * 5/9;
printf("%f fahreinheit is equal to %f celsius", F, C);
return 0;
}

Q16
